            <ITEXT FONTSIZE="10" CH=" from BrickPi import *           # import BrickPi.py file to use BrickPi"/>
            <para PARENT="Code"/>
            <ITEXT FONTSIZE="10" CH=" operationsBrickPiSetup()        # setup the serial port for communication"/>
            <para PARENT="Code"/>
            <ITEXT FONTSIZE="10" CH=" BrickPi.MotorEnable[PORT_A] = 1 # Enable the Motor A"/>
            <para PARENT="Code"/>
            <ITEXT FONTSIZE="10" CH=" BrickPiSetupSensors()           # Send the properties of sensors to BrickPi"/>
            <para PARENT="Code"/>
            <ITEXT FONTSIZE="10" CH=" while True:"/>
            <ITEXT CH="                     # Keep looping until the program is killed"/>
            <para PARENT="Code"/>
            <ITEXT FONTSIZE="10" CH="   print &quot;Running Forward&quot;"/>
            <para PARENT="Code"/>
            <ITEXT FONTSIZE="10" CH="   BrickPi.MotorSpeed[PORT_A] = 200  # Set the speed of MotorA (-255 to 255)"/>
            <para PARENT="Code"/>
            <ITEXT FONTSIZE="10" CH="   ot = time.time()"/>
            <para PARENT="Code"/>
            <ITEXT FONTSIZE="10" CH="   while(time.time() - ot &lt; 3):  # Run while loop for 3 seconds"/>
            <para PARENT="Code"/>
            <ITEXT FONTSIZE="10" CH="     BrickPiUpdateValues()       # Ask BrickPi to update values for sensors/motors"/>
            <para PARENT="Code"/>
