            <ITEXT FONT="FreeSans Bold" FONTSIZE="23" CH="Hardware/software co-design"/>
            <para LINESP="28"/>
            <ITEXT FONT="FreeSans Bold" FONTSIZE="23" CH="with LOGi-Pi"/>
            <trail LINESP="28"/>
